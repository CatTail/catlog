<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="chrome=1">
  <title><%= site_title %> by <%= author %></title>
  <link rel="stylesheet" href="<%= base_url %>theme/stylesheets/gfm_markdown.css">
  <link rel="stylesheet" href="<%= base_url %>theme/stylesheets/style.css">
  <!--[if lt IE 9]>
    <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
</head>
<body>
  <header id="header">
    <div class="aux">
      <h1><a href="/"><%= site_title %></a></h1>
    </div>
  </header>
  <nav id="nav">
    <ul>
      <li>
        <a href="/">Home</a>
      </li>
      <% categories.forEach(function (category) { %>
        <li>
        <a href="/<%= category %>"><%= category %></a>
        </li>
      <% }) %>
      <li>
        <a href="/feed.xml">Feed</a>
      </li>
    </ul>
  </nav>
